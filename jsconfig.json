    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@pages/*": ["src/pages/*"],
      "@types/*": ["src/types/*"],
      "@recoil/*": ["src/recoil/*"],
      "@hooks/*": ["src/hooks/*"],
      "@apis/*": ["src/apis/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
